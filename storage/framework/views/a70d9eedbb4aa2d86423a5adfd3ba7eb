    <script>
        function a() {
            var lis = document.getElementById("navLinks") || document.getElementById("unique");
            if (lis.id === 'navLinks') {
                lis.removeAttribute("id");
                lis.setAttribute("id", "unique");
            }
            else
                lis.setAttribute("id", "navLinks");
        }
    </script>
